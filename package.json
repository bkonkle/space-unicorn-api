{
  "name": "@space-unicorn/api",
  "version": "0.1.0",
  "description": "Space Unicorn",
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/space-unicorn-api.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us> (https://deterministic.dev)",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write",
    "format.all": "yarn format \"src/*.{js,jsx,ts,tsx}\"",
    "check.types": "tsc --pretty --noEmit --skipLibCheck",
    "db.migrate": "knex migrate:latest",
    "db.reset": "dropdb unicorn && createdb unicorn -O unicorn_root && yarn db.migrate",
    "db.shell": "PGPASSWORD=password psql -h localhost -U unicorn_root unicorn",
    "dev": "yarn dev.watch.debug",
    "dev.run": "NODE_ENV=development ts-node --transpile-only src/index.ts",
    "dev.debug": "DEBUG=unicorn-api:*,graphile-build:* yarn dev.run",
    "dev.models": "graphql-codegen",
    "dev.watch": "nodemon --exec yarn dev.run",
    "dev.watch.debug": "DEBUG=unicorn-api:*,graphile-build:* yarn dev.watch",
    "test.watch": "NODE_ENV=test jest --watch",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-noop": "^1.1.0",
    "express-promise-router": "^3.0.3",
    "graphile-build-pg": "^4.4.0",
    "graphile-utils": "^4.4.0",
    "graphql": "^14.3.1",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tag": "^2.10.1",
    "jwks-rsa": "^1.5.1",
    "knex": "^0.17.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "parse-database-url": "^0.3.0",
    "pg": "^7.11.0",
    "pg-sql2": "^2.2.3",
    "postgraphile": "^4.4.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "to-snake-case": "^1.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.2.1",
    "@graphql-codegen/typescript": "^1.2.1",
    "@graphql-codegen/typescript-operations": "^1.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.2.1",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/express-jwt": "^0.0.42",
    "@types/jest": "^24.0.15",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.8",
    "@types/ramda": "^0.26.9",
    "@types/react": "^16.8.20",
    "@types/to-snake-case": "^1.0.0",
    "debug": "^4.1.1",
    "dotenv-cli": "^2.0.0",
    "husky": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
